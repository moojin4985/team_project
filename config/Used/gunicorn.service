[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=hmj977_email
Group=www-data
WorkingDirectory=/home/hmj977_email/web/
ExecStart=/home/hmj977_email/web/venv/bin/gunicorn --workers 3 --bind unix:/home/hmj977_email/web/run/gunicorn.sock mysite.wsgi:application

[Install]
WantedBy=multi-user.target